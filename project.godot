[application]

config/name="A Sinister Time (RI Game Jam 2022)"
run/main_scene="res://MainScreen.tscn"
config/use_custom_user_dir=true
config/custom_user_dir_name="ASinisterTime"
boot_splash/image="res://Loading.png"
boot_splash/bg_color=Color( 0, 0, 0, 1 )
config/icon="res://icon.png"
config/windows_native_icon="res://icon.ico"

[autoload]

PlayerData="*res://Player/PlayerData.tscn"

[display]

window/size/width=320
window/size/height=180
window/size/fullscreen=true
window/size/test_width=1280
window/size/test_height=720
mouse_cursor/custom_image="res://assets/Cursors/Megabyte Games Mouse Cursor Pack/16x16/png dark/cursor-pointer-25-dark.png"
